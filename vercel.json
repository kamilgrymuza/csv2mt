{
  "rewrites": [
    {
      "source": "/",
      "destination": "/en/index.html"
    },
    {
      "source": "/en",
      "destination": "/en/index.html"
    },
    {
      "source": "/pl",
      "destination": "/pl/index.html"
    },
    {
      "source": "/:lang(en|pl)/:path*",
      "destination": "/index.html"
    }
  ]
}
